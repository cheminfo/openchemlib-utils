{
  "name": "openchemlib-utils",
  "version": "2.1.3",
  "description": "Various utilities that extends openchemlib-js like HOSE codes or diastereotopic IDs",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs && cheminfo-build --entry src/index.js --root OCLUtils",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "prepack": "rollup -c --bundleConfigAsCjs",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/openchemlib-utils.git"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/openchemlib-utils/issues"
  },
  "homepage": "https://github.com/cheminfo/openchemlib-utils#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@types/jest": "^29.2.5",
    "cheminfo-build": "^1.1.11",
    "eslint": "^8.32.0",
    "eslint-config-cheminfo": "^8.1.3",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.3.1",
    "jest-matcher-deep-close-to": "^3.0.2",
    "openchemlib": "8.0.0",
    "prettier": "^2.8.3",
    "rollup": "^3.10.0"
  },
  "dependencies": {
    "atom-sorter": "^2.0.0",
    "ensure-string": "^1.2.0",
    "ml-floyd-warshall": "^2.0.1",
    "ml-matrix": "^6.10.4",
    "papaparse": "^5.3.2",
    "sdf-parser": "^6.0.1"
  },
  "peerDependencies": {
    "openchemlib": ">=8.0.0"
  }
}
