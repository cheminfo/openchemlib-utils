{
  "name": "openchemlib-utils",
  "version": "1.11.0",
  "description": "Various utilities that extends openchemlib-js like HOSE codes or diastereotopic IDs",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c && cheminfo-build --entry src/index.js --root OCLUtils",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "prepare": "rollup -c",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheminfo/openchemlib-utils.git"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cheminfo/openchemlib-utils/issues"
  },
  "homepage": "https://github.com/cheminfo/openchemlib-utils#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@types/jest": "^28.1.6",
    "cheminfo-build": "^1.1.11",
    "eslint": "^8.22.0",
    "eslint-config-cheminfo": "^8.0.2",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "jest-matcher-deep-close-to": "^3.0.2",
    "openchemlib": "8.0.0",
    "prettier": "^2.7.1",
    "rollup": "^2.78.0"
  },
  "dependencies": {
    "atom-sorter": "^1.1.10",
    "ensure-string": "^1.2.0",
    "ml-floyd-warshall": "^1.0.4",
    "ml-matrix": "^6.10.2",
    "papaparse": "^5.3.2",
    "sdf-parser": "^5.0.1"
  },
  "peerDependencies": {
    "openchemlib": ">=8.0.0"
  }
}
